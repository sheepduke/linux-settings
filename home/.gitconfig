[color]
	status = auto
	branch = auto
	diff = auto
	interactive = auto
	ui = auto
	pager = true

[alias]
	br = branch
	co = checkout
	cm = commit
    cp = cherry-pick
    df = diff
    ds = diff --cached
    fa = fetch -a
    re = rebase
	st = status --ahead-behind
    stu = status --ahead-behind -uno
    lf = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    
[include]
    path = gituser.conf
